@model IEnumerable<VAK.Models.Employees>

<table class="border">
  <thead>
  <tr>
    <th>Employee ID</th>
    <th>Last Name</th>
    <th>First Name</th>
    <th>Title</th>
    <th>Title of Courtesy</th>
    <th>Birth Date</th>
    <th>Hire Date</th>
    <th>Address</th>
    <th>City</th>
    <th>Region</th>
    <th>Postal Code</th>
    <th>Country</th>
    <th>Home Phone</th>
    <th>Extension</th>
    <th>Notes</th>
    <th>Reports To</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  @foreach (var item in Model) {
    <tr>
      <td>@item.EmployeeId</td>
      <td>@item.LastName</td>
      <td>@item.FirstName</td>
      <td>@item.Title</td>
      <td>@item.TitleOfCourtesy</td>
      <td>@item.BirthDate</td>
      <td>@item.HireDate</td>
      <td>@item.Address</td>
      <td>@item.City</td>
      <td>@item.Region</td>
      <td>@item.PostalCode</td>
      <td>@item.Country</td>
      <td>@item.HomePhone</td>
      <td>@item.Extension</td>
      <td>@item.Notes</td>
      <td>@item.ReportsTo</td>
      <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.EmployeeId }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.EmployeeId })
      </td>
    </tr>
  }
  </tbody>
</table>

<p>@Html.ActionLink("Create New", "Create")</p>